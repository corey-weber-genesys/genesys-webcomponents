{
  "toast": {
    "close_button_color": "#626e84",
    "success": {
      "background_color": "#ffffff",
      "foreground_color": "#1b2c48",
      "icon_color": "#09b581"
    },
    "warning": {
      "background_color": "#ffffff",
      "foreground_color": "#1b2c48",
      "icon_color": "#f8c73e"
    },
    "info": {
      "background_color": "#ffffff",
      "foreground_color": "#1b2c48",
      "icon_color": "#6e5ddb"
